

{% block body %}

    
      {{form_start(form)}}
     
       <div class="row">
          <div class="col-md-6"> 
           
             {{form_row(form.firstName)}}
           
          </div>
          <div class="col-md-6">
           {{form_row(form.lastname)}}
           
          </div>
      </div>
      <div class="row">
          <div class="col-md-6">
            {{form_row(form.cin)}}
          </div>
          <div class="col-md-6">
              {{form_row(form.email)}}
              
          </div>
      </div>
     
      <div class="row">
        <div class="col-3">
        {{form_row(form.phone)}}
          
        </div>
        <div class="col-3">
        {{form_row(form.address)}}
          
        </div>
        <div class="col-3">
           {{form_row(form.registrationAt)}}
          
          </div>
          <div class="col-3">
           {{form_row(form.level)}}
          </div>
     </div>
     <div class="col-3" style="padding-bottom: 5px;">
    <button class="btn btn-primary ">{{ button_label|default('Enregistrer') }}</button>
     </div>
{{ form_end(form) }}
{% endblock %}